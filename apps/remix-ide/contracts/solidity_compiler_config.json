{
	"language": "Solidity",
	"settings": {
		"remappings": [
			":g=/dir"
		],
		"optimizer": {
			"enabled": true,
			"runs": 200,
			"details": {
				"peephole": true,
				"inliner": true,
				"jumpdestRemover": true,
				"orderLiterals": false,
				"deduplicate": false,
				"cse": false,
				"constantOptimizer": false,
				"yul": false
			}
		},
		"evmVersion": "byzantium",
		"viaIR": true,
		"debug": {
			"revertStrings": "default"
		},
		"metadata": {
			"useLiteralContent": true,
			"bytecodeHash": "ipfs"
		},
		"outputSelection": {
			"*": {
				"*": [
					"metadata",
					"evm.bytecode",
					"evm.bytecode.sourceMap"
				],
				"": [
					"ast"
				]
			},
			"def": {
				"MyContract": [
					"abi",
					"evm.bytecode.opcodes"
				]
			}
		},
		"modelChecker": {
			"divModNoSlacks": false,
			"engine": "chc",
			"showUnproved": true,
			"solvers": [
				"cvc4",
				"smtlib2",
				"z3"
			],
			"targets": [
				"underflow",
				"overflow",
				"assert"
			],
			"timeout": 20000
		}
	}
}